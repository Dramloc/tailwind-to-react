{
  "name": "tailwind-to-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:tailwind": "cross-env NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "watch:tailwind": "postcss -w src/tailwind.css -o src/index.css",
    "start:react": "react-scripts start",
    "prestart": "run-s build:tailwind",
    "start": "npm-run-all --parallel watch:tailwind start:react",
    "prebuild": "run-s build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@babel/core": "^7.12.7",
    "@babel/parser": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/traverse": "^7.12.7",
    "@codemod/core": "^1.1.0",
    "@headlessui/react": "^0.2.0",
    "@monaco-editor/react": "^3.7.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.0.2",
    "chalk": "^4.1.0",
    "clsx": "^1.1.1",
    "htmltojsx": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.8",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^2.26.3",
    "tailwindcss": "^2.0.1",
    "workerize-loader": "^1.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "postcss-cli": "^8.3.0",
    "react-scripts": "4.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "printWidth": 100
  },
  "babelMacros": {
    "twin": {
      "css": {
        "import": "css",
        "from": "@emotion/react"
      },
      "global": {
        "import": "Global",
        "from": "@emotion/react"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
